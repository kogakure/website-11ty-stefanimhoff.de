---
layout: false
permalink: manifest.webmanifest
ignore: true
---
{
  "name": "{{ site.title }}",
  "short_name": "{{ site.title }}",
  "start_url": "/",
  "icons": [
    {
      "src": "{{ site.faviconPath }}favicon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "{{ site.faviconPath }}favicon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "theme_color": "#e7e6e4",
  "background_color": "#e7e6e4",
  "display": "standalone"
}
