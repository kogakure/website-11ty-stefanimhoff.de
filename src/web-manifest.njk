		{
			"src": "{{ site.faviconPath }}favicon-512.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	],
	"theme_color": "#e7e6e4",
	"background_color": "#e7e6e4",
	"display": "standalone"
}
