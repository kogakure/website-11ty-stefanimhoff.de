{
	"name": "website-11ty-stefanimhoff.de",
	"version": "1.0.0",
	"description": "Personal website build with 11ty",
	"scripts": {
		"build": "ELEVENTY_ENV=production npm-run-all build:assets build:html build:sw",
		"build:assets": "gulp build",
		"build:html": "eleventy",
		"build:sw": "workbox generateSW workbox.config.js",
		"clean": "npx del dist",
		"debug": "DEBUG=* npx eleventy",
		"deploy": "netlify deploy --prod",
		"deploy:preview": "netflify deploy",
		"dev": "ELEVENTY_ENV=development npm run watch",
		"live": "netlify dev --live",
		"plop": "plop",
		"prebuild": "npm run clean",
		"prepare": "husky install",
		"prestart": "npm run clean",
		"publish": "npm-run-all build deploy",
		"start": "netlify dev",
		"svg": "gulp svg",
		"watch": "npm-run-all --parallel watch:*",
		"watch:assets": "gulp",
		"watch:html": "eleventy --serve"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kogakure/website-11ty-stefanimhoff.de.git"
	},
	"keywords": [
		"11ty"
	],
	"author": "Stefan Imhoff",
	"license": "ISC",
	"homepage": "https://www.stefanimhoff.de",
	"lint-staged": {
		"**/*.{md,html,css}": [
			"npx prettier --write"
		],
		"**/*.js": [
			"npx eslint --fix"
		]
	},
	"devDependencies": {
		"@11ty/eleventy": "1.0.2",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"autoprefixer": "^10.2.5",
		"babelify": "^10.0.0",
		"browserify": "^17.0.0",
		"cssnano": "^5.0.2",
		"del-cli": "^3.0.1",
		"eleventy-nbsp-filter": "^0.1.0",
		"eleventy-plugin-lazyimages": "^2.1.0",
		"eleventy-plugin-reading-time": "0.0.1",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"gulp": "^4.0.2",
		"gulp-imagemin": "^7.1.0",
		"gulp-plumber": "^1.2.1",
		"gulp-postcss": "^9.0.0",
		"gulp-size": "^3.0.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-svg-sprite": "^1.5.0",
		"html-minifier": "^4.0.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.0.3",
		"lodash": "^4.17.21",
		"markdown-it-charts": "^1.0.5",
		"markdown-it-external-anchor": "^1.0.0",
		"markdown-it-footnote": "^3.0.2",
		"markdown-it-github-headings": "^2.0.0",
		"markdown-it-spoiler": "^1.1.1",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"moment": "^2.29.1",
		"netlify-cli": "^3.39.4",
		"npm-run-all": "^4.1.5",
		"outdent": "^0.8.0",
		"plop": "^2.7.4",
		"postcss": "^8.2.13",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^8.3.1",
		"postcss-custom-media": "^8.0.0",
		"postcss-custom-properties": "^11.0.0",
		"postcss-extend": "^1.0.5",
		"postcss-import": "^14.0.0",
		"postcss-media-minmax": "^5.0.0",
		"postcss-nested": "^5.0.5",
		"postcss-sort-media-queries": "^3.7.7",
		"prettier": "^2.7.1",
		"sal.js": "^0.8.4",
		"stylelint": "^13.12.0",
		"stylelint-a11y": "^1.2.3",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-recommended": "^4.0.0",
		"stylelint-high-performance-animation": "^1.5.2",
		"stylelint-order": "^4.1.0",
		"stylelint-prettier": "^1.2.0",
		"uglify-js": "^3.13.3",
		"vinyl-buffer": "^1.0.1",
		"vinyl-source-stream": "^2.0.0",
		"workbox-cli": "^6.1.5"
	}
}
